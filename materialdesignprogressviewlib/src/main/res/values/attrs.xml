<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MaterialDesignProgressView">
        <attr name="circle_color" format="color" />
        <attr name="circle_line_width" format="dimension" />
        <attr name="circle_beigin_degree" format="integer" />
        <attr name="circle_radius" format="dimension" />
        <attr name="circle_running" format="boolean" />
        <attr name="circle_speed">
            <enum name="slow" value="2" />
            <enum name="medium" value="5" />
            <enum name="quick" value="10" />
        </attr>
    </declare-styleable>
</resources>